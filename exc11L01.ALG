Algoritmo "semnome"
//Leia 3 notas e calcule a média aritmética.
Var

  n1:real
  n2:real
  n3:real
  media:real

Inicio

   escreval("Informe o primeiro número")
   leia(n1)
   escreval("Informe o segundo número")
   leia(n2)
   escreval("Informe o terceiro número")
   leia(n3)
   
   media <- (n1+n2+n3)/3

   escreval("A média das notas é de:",media)

Fimalgoritmo