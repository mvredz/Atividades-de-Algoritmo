Algoritmo "semnome"
//Solicite dois valores e apresente a soma com acréscimo de 18%.
Var

   valor1:real
   valor2:real
   soma:real

Inicio

   escreval("Informe o primeiro valor")
   leia(valor1)
   escreval("Informe o segundo valor")
   leia(valor2)
   soma <- (valor1+valor2)*1.18
   escreval("Os valores somados e com acréscimo de 18% é igual a:",soma)

Fimalgoritmo