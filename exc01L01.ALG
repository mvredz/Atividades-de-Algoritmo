Algoritmo "semnome"
//Solicite o nome e o salário de uma pessoa. Calcule e apresente o novo salário com um aumento de 28,5%.
Var

  nome:caracter
  salario:real
  reajus:real

Inicio

   escreval("Informe o seu nome")
   leia(nome)
   escreval("Informe o seu salário")
   leia(salario)
   
   reajus <- salario * 1.285
   
   escreval("O novo salário ", reajus)

Fimalgoritmo